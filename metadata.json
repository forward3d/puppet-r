{
  "name": "forward3ddev-r",
  "version": "0.0.3",
  "author": "forward3d",
  "summary": "Install and manage R and its packages",
  "license": "Apache License, Version 2.0",
  "source": "https://github.com/forward3d/puppet-r.git",
  "project_page": "https://github.com/forward3d/puppet-r",
